<h1>Listing articles</h1>
 
<table>
  <tr>
    <th>Sender</th>
    <th>Body</th>
    <th>Original</th>
  </tr>
 
  <% @responses.each do |response| %>
    <tr>
      <td><%= response.sender %></td>
      <td><%= response.body %></td>
      <td><%= response.original %></td>
    </tr>
  <% end %>
</table>
